{
  "name": "farm-plugin-tspaths",
  "version": "0.1.2",
  "description": "Support for TypeScript's path mapping in Farm",
  "main": "./build/index.cjs",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./build/index.cjs",
      "import": "./build/index.cjs",
      "types": "./dist/src/index.d.ts",
      "require": "./build/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "url": "https://github.com/fairyscript/farm-plugin-tspaths",
    "directory": ".",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/fairyscript/farm-plugin-tspaths/issues"
  },
  "author": {
    "name": "FairyScript",
    "email": "h@queb.fun"
  },
  "scripts": {
    "build": "rimraf dist && farm build && tsc -p ./tsconfig.build.json",
    "start": "pnpm run build -w",
    "prepublishOnly": "npm run build",
    "type-check": "tsc -p ./tsconfig.build.json --noEmit"
  },
  "files": [
    "build",
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@farmfe/cli": "^1.0.0",
    "@farmfe/core": "^1.0.9",
    "@types/node": "^20.11.30",
    "jsonc-parser": "^3.2.1",
    "rimraf": "^5.0.5",
    "type-fest": "^4.14.0",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@farmfe/cli": ">=1.0.0",
    "@farmfe/core": ">=1.0.0",
    "jsonc-parser": "^3.2.0"
  }
}
